<!-- Modal -->
<div class="modal fade" v-for="(item, index) in siteData.portfolio.items" :id="'exampleModalLong'+index" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
  <div class="modal-dialog modal-lg" role="document" >
    <div class="modal-content" >
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle"></h5>
        <!-- <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <i class="fa fa-3x fa-times"></i>
        </button> -->
      </div>
      <div class="modal-body" >
        <div class="container text-center">
          <div class="row">
            <div class="col-lg-8 mx-auto">
              <h2 :show="item.titulo" class="text-secondary text-uppercase mb-0">{{item.titulo}}</h2>
              <hr v-if="((item.titulo)&&(item.titulo.trim()!=''))" class="star-dark mb-5">
              <img class="img-fluid mb-5" :src="item.img" alt="">
              <p v-if="item.txt" class="mb-5">{{item.txt}}</p>
              <br v-else>
              <a class="btn btn-primary btn-lg rounded-pill portfolio-modal-dismiss" href="#" data-dismiss="modal">
                <i class="fa fa-close"></i>
                Fechar</a>
            </div>
          </div>
        </div>
      </div>
      <div class="modal-footer">
      </div>
    </div>
  </div>
</div>
